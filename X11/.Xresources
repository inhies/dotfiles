! xscreensaver ---------------------------------------------------------------

!font settings
xscreensaver.Dialog.headingFont:        -*-dina-bold-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.bodyFont:           -*-dina-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.labelFont:          -*-dina-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.unameFont:          -*-dina-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.buttonFont:         -*-dina-bold-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.dateFont:           -*-dina-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.passwd.passwdFont:         -*-dina-bold-r-*-*-12-*-*-*-*-*-*-*
!general dialog box (affects main hostname, username, password text)
xscreensaver.Dialog.foreground:         #ffffff
xscreensaver.Dialog.background:         #111111
xscreensaver.Dialog.topShadowColor:     #111111
xscreensaver.Dialog.bottomShadowColor:  #111111
xscreensaver.Dialog.Button.foreground:  #666666
xscreensaver.Dialog.Button.background:  #ffffff
!username/password input box and date text colour
xscreensaver.Dialog.text.foreground:    #666666
xscreensaver.Dialog.text.background:    #ffffff
xscreensaver.Dialog.internalBorderWidth:24
xscreensaver.Dialog.borderWidth:        20
xscreensaver.Dialog.shadowThickness:    2
!timeout bar (background is actually determined by Dialog.text.background)
xscreensaver.passwd.thermometer.foreground:  #ff0000
xscreensaver.passwd.thermometer.background:  #000000
xscreensaver.passwd.thermometer.width:       8
!datestamp format--see the strftime(3) manual page for details
xscreensaver.dateFormat:    %I:%M%P %a %b %d, %Y

xscreensaver.passwd.heading.label: XScreenSaver %s
xscreensaver.passwd.body.label: This screen is locked.
xscreensaver.passwd.unlock.label: OK
xscreensaver.passwd.login.label: New Login
xscreensaver.passwd.user.label: Username:
xscreensaver.passwd.thermometer.width: 8
xscreensaver.passwd.asterisks: True
xscreensaver.passwd.uname: False

xscreensaver.lock: True

xscreensaver.mode: random
xscreensaver.timeout: 0:3:00
xscreensaver.cycle: 0:10:00
xscreensaver.lockTimeout: 0:00:5
xscreensaver.passwdTimeout: 0:00:5
xscreensaver.fade: False

! Hybrid Terminal Colours
! vim: ft=xdefaults
URxvt.perl-ext-common: default,clipboard,url-select
URxvt.keysym.C-M-u: perl:url-select:select_next

URxvt.keysym.C-M-c:   perl:clipboard:copy
URxvt.keysym.C-M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped

! URxvt.matcher.pattern.9: \[?([0-9a-f]{4}:){7}[0-9a-f]{4}\]?
URxvt.url-select.launcher:      /usr/bin/firefox
URxvt.url-select.button:   1
URxvt.url-select.underline: true
URxvt.colorUL: #268bd2


! URxvt.font: xft:Ubuntu Mono:size=11,xft:Monospace:size=9:antialias=true:hinting=true
! terminus dina
! URxvt.font:             xft:Terminus:pixelsize=14:antialias=false
! URxvt.boldFont:         xft:Terminus:bold:pixelsize=14:antialias=false 
! URxvt*font:xft:anonymouspro:size=10
URxvt*font:xft:terminus:size=9:antialias=true
 URxvt*boldFont:xft:terminus:bold:size=9:antialias=true
! urxvt*font: xft:DejaVu Sans Mono:size=10
 
!URxvt.letterSpace: -1

! Remove URxvt scrollbar
URxvt.scrollBar: false

